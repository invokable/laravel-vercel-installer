{
    "name": "revolution/laravel-vercel-installer",
    "description": "Vercel Installer",
    "keywords": [
        "laravel",
        "vercel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0||^13.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8",
        "laravel/pint": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\Vercel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\Vercel\\VercelInstallerServiceProvider"
            ]
        }
    }
}
